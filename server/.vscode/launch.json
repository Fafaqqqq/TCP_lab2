  "version": "0.2.0",
  "configurations": [
    {
      "name": "emulator dbg",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/example/bin/lab2",
      "args": [
        "--port",
        "1234",
        "--matrix",
        "5",
        "10",
        "--matrix",
        "10",
        "100"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/example/bin",
      "environment": [
        
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build"
    }
  ]
}
